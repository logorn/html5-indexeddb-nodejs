<% include ../layout/templates/header %>
<% include ../layout/templates/navbar %>
<% include ../layout/content/homeMessage %>

<div class="container" id="entityList">
    <div id="carPanel" class="panel panel-info">
        <div class="panel-heading"><h2>Car details </h2></div>
        <div class="panel-body">
            <div class="well">
                <ul>
                    <li>Car Maker : {{ car.maker }}</li>
                    <li>Car Model : {{ car.model }}</li>
                    <li>Car Year : {{ car.year }}</li>
                    <li>Car Driver : {{ car.driver }}</li>
                </ul>
            </div>
            <a v-bind:href="'../../car/edit/'+car.id">
                <button type="button" class="btn btn-info">Edit</button>
            </a>
            <a href="'../../../../car/">
                <button type="button" class="btn btn-info">Back to the list</button>
            </a>
        </div>
    </div>
</div>

<script>
    showCar(<%= id %>);
</script>

<% include ../layout/templates/footer %>